{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "create-admin": "node scripts/createAdmin.js",
    
    "_comment_deprecated": "Scripts obsolètes - remplacés par pageManager",
    "init-pages": "echo 'DEPRECATED: Utilisez npm run pages:init' && node scripts/pageManager.js init",
    "verify-pages": "echo 'DEPRECATED: Utilisez npm run pages:verify' && node scripts/pageManager.js verify",
    "manage": "echo 'DEPRECATED: Utilisez npm run pages:*' && node scripts/pageManager.js help",
    
    "_comment_new": "Nouveaux scripts unifiés avec pageManager",
    "pages:init": "node scripts/pageManager.js init",
    "pages:status": "node scripts/pageManager.js status",
    "pages:verify": "node scripts/pageManager.js verify",
    "pages:backup": "node scripts/pageManager.js backup",
    "pages:optimize": "node scripts/pageManager.js optimize",
    "pages:sync": "node scripts/pageManager.js sync",
    "pages:restore": "node scripts/pageManager.js restore",
    "pages:help": "node scripts/pageManager.js help",
    
    "_comment_aliases": "Aliases pour compatibilité",
    "pages:migrate": "npm run pages:init",
    "pages:cleanup": "npm run pages:optimize",
    
    "_comment_setup": "Script de setup complet",
    "setup": "npm run pages:init && npm run pages:verify && npm run pages:status",
    "setup:fresh": "npm run pages:backup && npm run pages:init && npm run pages:verify",
    
    "_comment_maintenance": "Scripts de maintenance",
    "maintenance:backup": "npm run pages:backup",
    "maintenance:verify": "npm run pages:verify && npm run pages:status",
    "maintenance:optimize": "npm run pages:optimize && npm run pages:verify"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "express": "~4.16.1",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "morgan": "~1.9.1",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}